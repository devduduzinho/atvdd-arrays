#include <stdio.h>

int main() {
    int numeros[10];

    for (int i = 0; i < 10; i++) {
        printf("Digite o %dº número: ", i + 1);
        scanf("%d", &numeros[i]);
    }

    printf("Números digitados: ");
    for (int i = 0; i < 10; i++) {
        printf("%d ", numeros[i]);
    }

    return 0;
}
